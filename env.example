## Copy to .env and fill in your bot tokens

# Required tokens
CLIPPY_DISCORD_TOKEN=
MUSIC_DISCORD_TOKEN=
MTG_DISCORD_TOKEN=

# Optional (common)
LOG_LEVEL=info
DEBUG=false

# Music bot (optional overrides)
MUSIC_MAX_QUEUE_SIZE=100
MUSIC_INACTIVITY_TIMEOUT=300
MUSIC_VOLUME_LEVEL=0.5

# MTG bot (optional overrides)
MTG_CACHE_TTL=3600
MTG_CACHE_SIZE=1000
